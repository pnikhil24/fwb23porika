extends layout

block content
  h1= title
  - let image_names = ['costmilk.png','milkprepare.jpg','milktypes2.jpg','quantity.jpg','Typesofmilk.png']
  - for(let i=0;i<3;i++){
      - let rno = Math.floor(Math.random()*5)
      - let iname = image_names[rno]
      img(src=`images/${iname}` alt='Milk' style='width:300px; height:300px;')
  - }
  
  
